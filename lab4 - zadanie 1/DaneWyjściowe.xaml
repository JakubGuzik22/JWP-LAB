<Window x:Class="lab4___zadanie_1.DaneWyjściowe"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lab4___zadanie_1"
        mc:Ignorable="d"
        Title="DaneWyjściowe" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="tbkPole" HorizontalAlignment="Left" Margin="51,23,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Height="152" Width="159" Background="#FFE84949" x:FieldModifier="private"/>
        <TextBlock x:Name="tbkObwód" HorizontalAlignment="Left" Margin="251,23,0,0" TextWrapping="Wrap" Text="0" VerticalAlignment="Top" Height="149" Width="181" RenderTransformOrigin="0.5,0.5" Background="#FF48A25E"  x:FieldModifier="private">
            <TextBlock.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </TextBlock.RenderTransform>
        </TextBlock>
        <Button x:Name="btnOkej" Content="OK" HorizontalAlignment="Left" Margin="200,201,0,0" VerticalAlignment="Top" Height="73" Width="166" Click="btnOkej_Click"/>

    </Grid>
</Window>
